package Test;

public class Lanceur {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Personne francis = new Personne("Francis", 25);
		
		// System.out.println(francis.nom + " " + francis.age); Inaccessible car private
		// francis.age = -6; On met en private pour éviter ceci
		
		francis.marcher();
		
		System.out.println(francis.getNom() + " " + francis.getAge());
		
		francis.setAge(-6);
		
		System.out.println(francis.getNom() + " " + francis.getAge());
	}

}
